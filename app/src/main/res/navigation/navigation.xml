<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/hitungFragment">

    <fragment
        android:id="@+id/hitungFragment"
        android:name="com.d3if3071.hitungbmi.ui.HitungFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_hitung">
        <action
            android:id="@+id/action_hitungFragment_to_saranFragment"
            app:destination="@id/saranFragment" />
        <action
            android:id="@+id/action_hitungFragment_to_aboutFragment2"
            app:destination="@id/aboutFragment" />

    </fragment>
    <fragment
        android:id="@+id/saranFragment"
        android:name="com.d3if3071.hitungbmi.ui.SaranFragment"
        tools:layout="@layout/fragment_saran">
        <argument
            android:name="kategori"
            app:argType="com.d3if3071.hitungbmi.model.KategoriBmi" />
    </fragment>
    <fragment
        android:id="@+id/aboutFragment"
        android:name="com.d3if3071.hitungbmi.AboutFragment"
        android:label="@string/tentang_aplikasi"
        tools:layout="@layout/fragment_about"/>

</navigation>